insert into usuario (nome, senha) values ("maria", "1234567"); -- inseri usuario 

 
insert into criptomoeda (nome) values ("eth"); -- inseri criptomoeda

update criptomoeda set nome = "venom"  where nome = "sol"; -- atualiza

delete from criptomoeda where nome = "btc";  -- deleta

insert into procurar (search, nome) values (2, "eth"); -- procurar cripto

